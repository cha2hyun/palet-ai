{
  "name": "palet-ai",
  "version": "1.0.7",
  "license": "MIT",
  "main": "dist-electron/index.js",
  "author": {
    "name": "@cha2hyun"
  },
  "description": "Use multiple AI services (ChatGPT, Gemini, Perplexity) simultaneously in one beautiful desktop app.",
  "keywords": [
    "ai",
    "chatgpt",
    "gemini",
    "perplexity",
    "multi-ai",
    "electron",
    "desktop-app",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cha2hyun/palet-ai.git"
  },
  "homepage": "https://github.com/cha2hyun/palet-ai",
  "productName": "Palet AI",
  "scripts": {
    "dev": "vite",
    "dev:electron": "yarn build:electron && electron .",
    "build": "yarn build:vite && yarn build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "yarn build && electron-builder",
    "dist:win": "yarn build && electron-builder --win --ia32",
    "dist:win:x64": "yarn build && electron-builder --win --x64",
    "dist:mac": "yarn build && electron-builder --mac --dir",
    "dist:mac:arm64": "yarn build && electron-builder --mac --arm64 --dir",
    "dist:mac:x64": "yarn build && electron-builder --mac --x64 --dir",
    "dist:mac:universal": "yarn build && electron-builder --mac --universal --dir",
    "dist:linux": "yarn build && electron-builder --linux",
    "pack": "yarn build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@heroui/react": "^2.8.5",
    "@vitejs/plugin-react": "^4.3.4",
    "electron-is-dev": "^2.0.0",
    "framer-motion": "^12.23.24",
    "i18next": "^23.11.5",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^14.1.2",
    "vite": "^5.4.11",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.4",
    "electron": "^30.0.7",
    "electron-builder": "^24.13.3",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^5.7.2"
  },
  "build": {
    "appId": "com.paletai.app",
    "productName": "Palet AI",
    "asar": true,
    "asarUnpack": [
      "**/*.node"
    ],
    "compression": "maximum",
    "icon": "resources/icon.png",
    "files": [
      "dist-electron",
      "dist-vite",
      "!node_modules",
      "!**/*.map"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "mac": {
      "icon": "resources/icon.icns",
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dir",
          "arch": [
            "arm64"
          ]
        }
      ],
      "identity": null
    },
    "win": {
      "icon": "resources/icon.ico",
      "target": [
        {
          "target": "portable",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "resources/icon.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
